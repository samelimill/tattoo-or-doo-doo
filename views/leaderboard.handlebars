<div class="w-1/4 h-screen bg-black text-white p-4 pt-10 absolute z-10" id="leaderboard">
  <button id="closeLeaderboard" class="absolute top-2 right-2 text-white text-2xl pr-2 cursor-pointer">X</button>
  <h2 class="text-2xl font-semibold mb-4">Leaderboard</h2>
  <div class="bg-black text-white p-4">
    <div class="flex items-center justify-between">
      <span class="text-yellow-200">Rank</span>
      <span class="text-yellow-200">Name</span>
      <span class="text-yellow-200">Rank Score</span>
    </div>
  </div>
  <div class="bg-yellow-200 h-1 mt-2"></div>
</div>

  {{#each users}}
<div id="container">



